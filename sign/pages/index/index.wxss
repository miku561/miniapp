.container{
  position: relative;
  height: 100%;
  text-align: center
}
.container .avatar{
  display: inline-block;
  margin: 80rpx auto 30rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
}
.list-wrap{
  width: 80%;
  height: 800rpx;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  margin: 0 auto;
}
icon{
  vertical-align: baseline;
}
.weui-check__label{
  position: relative;
  margin-bottom: 20rpx;
  border-radius: 8rpx;
  border: 1rpx solid #ccc;
  background: #6fc;
  color: #444;
  font-size: 32rpx;
}
.weui-check__label .wrap{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.weui-check__label .icon{
  border: 1rpx solid #ccc;
  border-radius: 50%;
  width: 36rpx;
  height: 36rpx;
}
.weui-check__label .icon.isShow{
  color: #fff
}
.fixed-btn{
  position: fixed;
  bottom: 50rpx;
  border-radius: 50%;
  box-shadow: 0 0 10rpx 5rpx rgba(0, 0, 0, 0.2);
  width: 100rpx;
  height: 100rpx;
  background: #39c5bb;
  text-align: center;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #fff;
}
.date-btn{
  left: 40rpx;
}
.add-btn{
  right: 40rpx;
}