.data-item{
  position: relative;
  padding: 30rpx;
  border-top: 1rpx solid #d0d0d0;
  font-size: 32rpx;
  -webkit-overflow-scrolling: touch;
}
.data-item .mask{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  height: 100%;
}
.data-item .nums{
  position: absolute;
  top: 0;
  left: 0;
  color: #666;
  font-size: 32rpx;
}
.data-item .imgs{
  flex: 0 0 120rpx;
  margin-left: 20rpx;
  width: 120rpx;
  height: 120rpx;
}